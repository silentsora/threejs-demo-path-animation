<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <!--[if IE]>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <![endif]-->
    <title>Treedom</title>
    <meta id="share-name" itemprop="name" content="">
    <meta id="share-description" itemprop="description" content="">
    <meta id="share-image" itemprop="image" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="format-detection" content="telephone=no" />
    <meta name="apple-mobile-web-app-capable" content="yes">
    
    <!-- rem布局初始化设置start -->
    <script type="text/javascript">
        <%= require('raw-loader!./src/js/app/rem.js') %>
    </script>
    <!-- rem布局初始化设置end -->

    <% if (process.env.NODE_ENV === 'prod') { %>
        <% for (var css in htmlWebpackPlugin.files.css) { %>
            <% var cssFile = htmlWebpackPlugin.files.css[css].replace("/css", "/img"); %>
            <link href="<%= cssFile %>" rel="stylesheet">
        <% } %> 
    <% } else { %>
        <% for (var css in htmlWebpackPlugin.files.css) { %>
            <% var cssFile = htmlWebpackPlugin.files.css[css] %>
            <link href="<%= cssFile %>" rel="stylesheet">
        <% } %>
    <% } %>

    <!-- 授权加代码统计系统 -->
    <!-- <script id="__ga" type="text/javascript" src="https://ga.treedom.cn/ga.js?v=0.3.4&pid=9999"></script> -->
</head>
<body>
    <div class="m-main-wrap absf">
        
        <!-- 竖屏页 -->
        <div class="m-vertical absf">
            <div class="m-wrap sizcover abcter">
                <!-- 业务逻辑代码 stat -->

                
                <!-- 业务逻辑代码 end -->
            </div>
        </div>

        <!-- 横屏页 -->
        <div class="m-horizontal absf">
            <div class="m-wrap sizcover abcter">
                <!-- 业务逻辑代码 stat -->

                <div class="m-loading">
                    <div class="btn-start"></div>
                    <div class="video-loading"></div>
                </div>
                <div class="m-scene">
                </div>
                <div class="m-ui">
                    <div class="btn-switch-view">切换视角</div>
                </div>
                
                <!-- 业务逻辑代码 end -->
            </div>
        </div>

        <!-- 前置加载 -->
        <div class="pre-loading absf">
            <div class="pre-loading-circle"></div>
        </div>
        <div class="simple-msg"></div>
    </div>

    <!-- 微信分享JSDK 需要时打开 -->
    <!-- <script src="//res.wx.qq.com/open/js/jweixin-1.2.0.js"></script> -->

    <!--  正式上线可替换此zepto（腾讯CDN） -->
    <!-- <script src="//cdnjs.gtimg.com/cdnjs/libs/zepto/1.0rc1/zepto.min.js"></script> -->

    <!-- 调试工具 -->
    <script src="<%= require('./src/js/lib/vconsole.min.js') %>"></script>
    <script>
        var vc = new VConsole();
    </script>
    <!-- <script src="<%= require('./src/js/lib/three_v113.min.js') %>"></script> -->
    <!-- <script src="<%= require('./src/js/lib/OrbitControls.js') %>"></script> -->
    <!-- <script src="<%= require('./src/js/lib/dat.gui.min.js') %>"></script> -->

    <!-- 本地zepto -->
    <!-- <script src="<%= require('./src/js/lib/zepto.min.1.2.js') %>"></script> -->

    <% if (process.env.NODE_ENV === 'prod') { %>
        <% for (var chunk in htmlWebpackPlugin.files.chunks) { %>
            <% var jsFile = htmlWebpackPlugin.files.chunks[chunk].entry.replace("/js", "/img"); %>
            <script type="text/javascript" src="<%= jsFile %>"></script>
        <% } %>
    <% } else { %>
        <% for (var chunk in htmlWebpackPlugin.files.chunks) { %>
            <% var jsFile = htmlWebpackPlugin.files.chunks[chunk].entry; %>
            <script type="text/javascript" src="<%= jsFile %>"></script>
        <% } %>
    <% } %>

    </body>
</html>
